/ {
	/* List of SoCs that this device tree is compatible with */
	soc_compatible {
		B0_IPOP {
			description = "B0,IPOP";
			product_id = <0x09855>;
			major = <0x1>;
			minor = <0x0>;
			pkg_mode = <0x1>;
		};
	};
};

